def plus(a=0, b=0):
    print(a + b)

def minus(a=0, b=0):
    print(a - b)

def multiplication(a=0, b=0):
    print(a * b)

def division(a=0, b=1):
    print(a / b)

def power(a=0, b=1):
    print(a ** b)

plus(3, 5)
minus(7 , 2)
multiplication(2, 4)
division(9, 3)

plus(1.5, -20)
minus(2, 7)
multiplication(-5, -2)
division(2, 8)

division(1)
power(50)